{
  "name": "political-contribution-monitor",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "version": "1.0.0",
  "description": "Political Contribution Monitoring System - Full Stack Application",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm start",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "process-data": "cd backend && npm run process-data"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.57.1"
  },
  "keywords": [
    "political",
    "contributions",
    "compliance",
    "fec",
    "monitoring",
    "react",
    "nodejs"
  ],
  "author": "Political Contribution Monitor Team",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.2.0"
  }
}
